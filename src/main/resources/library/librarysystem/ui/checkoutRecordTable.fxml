<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane minHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/9.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="library.librarysystem.controller.CheckoutRecordEntryController">
    <children>
        <TableView fx:id="table" layoutX="14.0" layoutY="14.0" prefHeight="300.0" prefWidth="500.0"
                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="isbn" prefWidth="75.0" text="ISBN"/>
                <TableColumn fx:id="title" prefWidth="75.0" text="Title"/>
                <TableColumn fx:id="copyNum" prefWidth="75.0" text="Copy Num"/>
                <TableColumn fx:id="checkoutDate" prefWidth="75.0" text="Checkout Date"/>
                <TableColumn fx:id="dueDate" prefWidth="75.0" text="Due Date"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>