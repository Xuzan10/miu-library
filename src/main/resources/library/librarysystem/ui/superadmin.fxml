<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="479.0" prefWidth="574.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="library.librarysystem.controller.SuperAdminController">
    <children>
        <VBox layoutX="-8.0" prefHeight="26.0" prefWidth="738.0">
            <children>
                <MenuBar prefHeight="26.0" prefWidth="584.0">
                    <menus>
                        <Menu mnemonicParsing="false" text="User">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#logout" text="Logout"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </children>
        </VBox>
        <VBox layoutY="26.0" prefHeight="450.0" prefWidth="730.0">
            <children>
                <TabPane prefHeight="450.0" prefWidth="636.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab text="Member">
                            <content>
                                <AnchorPane fx:id="memberAnchor" minHeight="0.0" minWidth="0.0" prefHeight="422.0"
                                            prefWidth="699.0">
                                    <children>
                                        <Button layoutX="14.0" layoutY="25.0" mnemonicParsing="false"
                                                onAction="#addMember" text="Add Member"/>
                                        <TableView fx:id="memberTable" layoutX="-1.0" layoutY="61.0" prefHeight="359.0"
                                                   prefWidth="730.0">
                                            <columns>
                                                <TableColumn fx:id="id" prefWidth="52.800018310546875" text="id"/>
                                                <TableColumn fx:id="name" prefWidth="118.39999389648438" text="Name"/>
                                                <TableColumn fx:id="phone" prefWidth="90.39996337890625" text="Phone"/>
                                                <TableColumn fx:id="street" prefWidth="163.199951171875" text="Street"/>
                                                <TableColumn fx:id="city" prefWidth="134.4000244140625" text="City"/>
                                                <TableColumn fx:id="state" minWidth="-1.0" prefWidth="72.800048828125"
                                                             text="State"/>
                                                <TableColumn fx:id="zip" minWidth="0.0" prefWidth="95.20004882812498"
                                                             text="Zip"/>
                                            </columns>
                                        </TableView>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                        <Tab text=" Books">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="425.0" prefWidth="586.0">
                                    <children>
                                        <Button layoutX="122.0" layoutY="27.0" mnemonicParsing="false"
                                                text="Checkout Books"/>
                                        <Button layoutX="20.0" layoutY="27.0" mnemonicParsing="false"
                                                onAction="#addBook" text="Add Books"/>
                                        <TableView fx:id="bookTable" layoutY="69.0"
                                                   prefHeight="354.0" prefWidth="730.0">
                                            <columns>
                                                <TableColumn fx:id="isbn" prefWidth="82.39999079704285" text="ISBN"/>
                                                <TableColumn fx:id="bookName" prefWidth="200.8000030517578"
                                                             text="Book Name"/>
                                                <TableColumn fx:id="copyNum" minWidth="0.0"
                                                             prefWidth="81.60003662109375" text="Copies"/>
                                                <TableColumn fx:id="maxCheckout" minWidth="0.0"
                                                             prefWidth="100.800048828125" text="Max Checkout"/>
                                                <TableColumn fx:id="authors" minWidth="7.20001220703125"
                                                             prefWidth="261.5999816894531" text="Authors"/>
                                            </columns>
                                        </TableView>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
