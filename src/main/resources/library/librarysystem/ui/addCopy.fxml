<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="201.0" prefWidth="239.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="library.librarysystem.controller.AddCopyController">
    <children>
        <Label layoutX="34.0" layoutY="43.0" text="Number of copies"/>
        <TextField fx:id="numCopy" layoutX="34.0" layoutY="67.0"/>
        <Button layoutX="68.0" layoutY="129.0" mnemonicParsing="false" onAction="#addCopy" prefHeight="26.0"
                prefWidth="82.0" text="Add Copy"/>
        <Label fx:id="message" layoutX="35.0" layoutY="93.0" style="-fx-text-fill: #ff0000;">
            <font>
                <Font size="10.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
