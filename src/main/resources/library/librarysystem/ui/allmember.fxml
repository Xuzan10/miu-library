<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="anchor"  maxHeight="450.0" prefHeight="450.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="library.librarysystem.controller.AllMemberController">
    <children>
        <Label fx:id="allMemberLabel" layoutX="30.0" layoutY="32.0" prefHeight="42.0" prefWidth="150.0"
               text="All Member IDs">
            <font>
                <Font name="Harlow Solid Italic" size="20.0"/>
            </font>
        </Label>
        <Button layoutX="30.0" layoutY="326.0" mnemonicParsing="false" onAction="#onBackPressed"
                text="&lt;= Back to Main"/>
        <TextArea fx:id="textArea" layoutX="30.0" layoutY="100.0" maxWidth="400.0"/>
    </children>
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
    </padding>
</AnchorPane>
